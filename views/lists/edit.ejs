<% layout('layouts/boilerplate') %> 

<h1>Edit List</h1>
<form action="/lists/<%= list._id %>?_method=PUT" method="POST">
    <div>
        <label for="title">Title: </label>
        <input type="text" name="list[title]" id="title" value="<%= list.title %>">
    </div>
    <button type="submit">Edit List</button>
    <a href="/lists/<%= list._id %> ">Back</a>
</form>
<form action="/lists/<%= list._id %>?_method=DELETE" method="POST">
    <button type="submit">Delete List</button>
</form>
